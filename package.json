{
  "name": "v3-recaptcha",
  "version": "0.0.13",
  "description": "Easily incorporate V3 recaptcha from google into your application",
  "author": "PlusTimeIT <kane@plustime.com.au> (https://plustime.com.au/)",
  "type": "module",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build": "./.scripts/build.sh --non-interactive",
    "build:vite": "rimraf dist && vite build",
    "build:types": "vue-tsc --project tsconfig.build-types.json --declaration"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/PlusTimeIT/v3-recaptcha.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vue3",
    "google",
    "google recaptcha",
    "recaptcha V3"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PlusTimeIT/v3-recaptcha/issues"
  },
  "homepage": "https://github.com/PlusTimeIT/v3-recaptcha#readme",
  "devDependencies": {
    "vue": "^3.4.19",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vue-tsc": "^1.8.27",
    "vite": "^5.1.3"
  },
  "dependencies": {},
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  }
}
